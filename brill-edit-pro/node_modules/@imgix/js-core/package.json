{
  "name": "@imgix/js-core",
  "description": "A JavaScript client library for generating image URLs with imgix",
  "version": "v3.1.3",
  "repository": "https://github.com/imgix/js-core",
  "license": "BSD-2-Clause",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/imgix-js-core.d.ts",
  "dependencies": {
    "js-base64": "~2.6.0",
    "md5": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.12",
    "@babel/register": "7.13.8",
    "benchmark": "2.1.4",
    "esm": "^3.2.25",
    "mocha": "8.3.2",
    "prettier": "2.2.1",
    "rollup": "2.42.4",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-uglify": "6.0.4",
    "sinon": "10.0.0",
    "typescript": "4.2.3",
    "uglify-js": "3.13.2"
  },
  "scripts": {
    "assert_version": "node assert_version.cjs",
    "build": "rollup -c",
    "compile": "cp ./src/imgix-js-core.d.ts ./dist/imgix-js-core.d.ts && tsc",
    "dev": "rollup -c -w",
    "prepare": "npm run build && npm run compile && npm run assert_version",
    "pretest": "npm run build",
    "pretty": "prettier --write '{src,test}/**/*.js'",
    "test": "mocha --require esm --recursive ./test/*.js",
    "test:performance": "mocha --require esm --recursive test/performance/*.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "src"
  ]
}
